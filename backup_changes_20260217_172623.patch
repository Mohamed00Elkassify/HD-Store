+  useEffect(() => {
+    setMounted(true);
+  }, []);
+
+  const itemCount = mounted ? getItemCount() : 0;
+
+  const navLinks = [
+    { href: "/", label: t("home") },
+    { href: "/products", label: t("products") },
+    { href: "/offers", label: t("offers") },
+    { href: "/faq", label: t("faq") },
+    { href: "/about", label: t("about") },
+    { href: "/contact", label: t("contact") },
