+                erp_error = str(exc)
+
+            # --- 2) Create local Order record ---
+            local_order = None
+            try:
+                local_order = create_local_order(
+                    customer_name=cd["full_name"],
+                    phone=cd["phone"],
+                    center=center_label,
+                    address=cd["address_details"],
