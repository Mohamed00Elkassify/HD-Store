+                logger.error("Local order creation failed: %s", exc)
+
+            # --- 3) Build WhatsApp message ---
+            order_message = _build_order_message(
+                cd, cart_items, cart_total, lang, t,
+                erp_so_name=erp_so_name,
+            )
+            wa_link = get_whatsapp_link(order_message)
